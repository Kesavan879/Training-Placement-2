class Solution {
    public int[] numberOfPairs(int[] nums) {
        int n = nums.length;
        int pair=0;
        int left=0;
        //Arrays.sort(nums);
        Map<Integer, Integer> freq = new HashMap<>(); 
        for (int num : nums) { 
            freq.put(num, freq.getOrDefault(num, 0) + 1); 
        }
        for(int v:freq.values()){
            pair+=(v/2);
            left+=(v%2);
        }
        int[] res = {pair, left};

        return res;
        
    }
}
