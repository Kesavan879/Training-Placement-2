class Solution {
    public int[] answerQueries(int[] nums, int[] queries) {
        Arrays.sort(nums);
        for(int i = 1; i < nums.length; i++) {
            nums[i] = nums[i - 1] + nums[i];
        }
        
        for(int i = 0; i < queries.length; i++) {
            int count = 0;
            for(int j = 0; j < nums.length; j++) {
                if(nums[j] <= queries[i]) 
                    count++;
                else
                    break;
            }
            queries[i] = count;
        }
        return queries;
    }
}
