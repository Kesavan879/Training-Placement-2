class Solution(object):
    def duplicateZeros(self, arr):
        cnt = 0
        for i in range(len(arr)):
            if arr[i] == 0:
                cnt += 1
                arr[-cnt] = -1
                if i + cnt == len(arr):
                    cnt -= 1
        for i in range(len(arr) - 1, -1, -1):
            if arr[i] > 0:
                arr[i], arr[i + cnt] = arr[i + cnt], arr[i]
            elif arr[i] == 0:
                cnt -= 1
        for i in range(len(arr)):
            if arr[i] == -1:
                arr[i] = 0
