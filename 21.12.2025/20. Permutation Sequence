// semicolons : 1
class Solution { public String getPermutation(int n, int k) { return ((StringBuilder) IntStream.range(0, n).map(i -> n - 1 - i).boxed().reduce(new Object[]{IntStream.rangeClosed(1, n).boxed().collect(Collectors.toList()), IntStream.range(0, n).map(j -> IntStream.rangeClosed(1, j).reduce(1, (a, b) -> a * b)).toArray(), new int[]{k - 1}, new StringBuilder()}, (a, i) -> ((StringBuilder) ((Object[]) a)[3]).append(((List<Integer>) ((Object[]) a)[0]).remove(((int[]) ((Object[]) a)[2])[0] / ((int[]) ((Object[]) a)[1])[i])) != null && (((int[]) ((Object[]) a)[2])[0] %= ((int[]) ((Object[]) a)[1])[i]) >= 0 ? a : a, (a, b) -> a)[3]).toString(); } }
