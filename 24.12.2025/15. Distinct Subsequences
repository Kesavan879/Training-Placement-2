class Solution {

    public int recu(String s, String t, int m, int n) {
        if (n == 0)
            return 1;

        if (m == 0)
            return 0;

        int count = 0;

        if (s.charAt(m - 1) == t.charAt(n - 1)) {
            int take = recu(s, t, m - 1, n - 1);
            int notTake = recu(s, t, m - 1, n);

            count = take + notTake;
        } else {
            count += recu(s, t, m - 1, n);
        }

        return count;
    }

    public int memoization(String s, String t, int[][] dp, int m, int n) {
        if (n == 0)
            return 1;

        if (m == 0)
            return 0;

        if (dp[m - 1][n - 1] != -1)
            return dp[m - 1][n - 1];

        int count = 0;

        if (s.charAt(m - 1) == t.charAt(n - 1)) {
            int take = memoization(s, t, dp, m - 1, n - 1);
            int notTake = memoization(s, t, dp, m - 1, n);

            count = take + notTake;
        } else {
            count += memoization(s, t, dp, m - 1, n);
        }

        return dp[m - 1][n - 1] = count;
    }

    public int memoizationUtil(String s, String t) {
        int m = s.length();
        int n = t.length();

        int[][] dp = new int[m][n];

        for (int[] d : dp) {
            Arrays.fill(d, -1);
        }

        return memoization(s, t, dp, m, n);
    }

    public int tabulation(String s, String t) {
        int m = s.length();
        int n = t.length();

        int[][] dp = new int[m + 1][n + 1]; 

        for(int i = 0; i < m + 1; i++) {
            dp[i][0] = 1;
        }

        for(int i = 1; i <= m; i++) {
            for(int j = 1; j <= n; j++) {
                if(s.charAt(i-1) == t.charAt(j-1)) {
                    dp[i][j] = dp[i-1][j-1] + dp[i-1][j];
                } else {
                    dp[i][j] = dp[i-1][j];
                }
            }
        }

        return dp[m][n];
    }

    public int spaceOptimization(String s, String t) {
        int m = s.length();
        int n = t.length();

        int[] dp = new int[n + 1]; 

        dp[0] = 1;

        for(int i = 1; i <= m; i++) {
            int[] curr = new int[n + 1];
            curr[0] = 1;

            for(int j = 1; j <= n; j++) {
                if(s.charAt(i-1) == t.charAt(j-1)) {
                    curr[j] = dp[j-1] + dp[j];
                } else {
                    curr[j] = dp[j];
                }
            }

            dp = curr;
        }

        return dp[n];
    }

    public void print(int[][] arr) {
        for(int a[] : arr) {
            for(int n : a) {
                System.out.print(n + " ");
            }
            System.out.println();
        }
    }

    public int numDistinct(String s, String t) {

        return spaceOptimization(s, t);
    }
}
