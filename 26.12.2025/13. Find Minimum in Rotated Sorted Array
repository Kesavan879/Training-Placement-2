class Solution {
    public int findMin(int[] nums) {
        int low = 0;
        int high = nums.length-1;
        
        while(low <= high){
            int mid = (high - low)/2 + low;
            //completely sorted
            if(nums[low] <= nums[mid] && nums[mid] <= nums[high]){
                return nums[low];
            }
            if(nums[mid] <= nums[high]){
                //right sorted, so min will be on left 
                // or can be mid, Case [3,1,2]
                // Eliminate Right!
                high = mid;
            }else{
                //left sorted but not completely sorted, so look on right!
                // Eliminate Left!
                low = mid+1;
            }
        }
        return -1;
    }
}
