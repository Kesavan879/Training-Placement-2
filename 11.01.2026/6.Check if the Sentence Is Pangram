class Solution():

    def checkIfPangram(self, sentence):
        List=['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']
        Letters_in_Sentence = list(sentence)
        
        condition=True
        for ch in List:
            if ch in Letters_in_Sentence :
                continue
            else:
                condition = False
                break
        return condition

p1 = Solution()
p1.checkIfPangram("thequickbrownfoxjumpsoverthelazydog")
