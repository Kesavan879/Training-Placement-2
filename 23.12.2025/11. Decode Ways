class Solution {
    public int numDecodings(String s) {
        if (s.length() == 1) {
            return s.charAt(0) != '0' ? 1 : 0;
        }
        int[] dp = new int[s.length()];
        if (s.charAt(0) != '0') {
            dp[0] = 1;
        }
        else {
            dp[0] = 0;
        }
        if (s.charAt(1) != '0') {
            dp[1] += dp[0];
        }
        int mid = (s.charAt(0) - '0') * 10 + (s.charAt(1) - '0'); 
        if (mid <= 26 && mid >= 10) {
            dp[1]++;
        }
        for (int i = 2; i < s.length(); i++) {
            if (s.charAt(i) != '0') {
                dp[i] += dp[i - 1];
            }
            int m = (s.charAt(i - 1) - '0') * 10 + (s.charAt(i) - '0');
            if (m <= 26 && m >= 10) {
                dp[i] += dp[i - 2];
            }
        }
        return dp[s.length() - 1];
        
    }
}
